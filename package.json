{
  "name": "loggee",
  "version": "0.1.2",
  "description": "Zero-dependency console logger with prefixed output and log-level muting",
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/loggee.git"
  },
  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "scripts": {
    "code": "eslint *.js",
    "test": "mocha test.js",
    "prerelease": "npm run code && npm t",
    "release": "npm version $VER && npm publish",
    "postrelease": "git push --follow-tags --no-verify",
    "release-patch": "VER=patch npm run release",
    "release-minor": "VER=minor npm run release",
    "precommit": "lint-staged && npm t",
    "prepush": "npm run code && npm t"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "devDependencies": {
    "eslint": "^4.15.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "sinon": "^6.0.1"
  },
  "keywords": [
    "logger"
  ],
  "license": "MIT"
}
